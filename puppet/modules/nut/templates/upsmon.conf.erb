# Created by puppet on <%= servername %> - do not edit here

DEADTIME	15
FINALDELAY	5
HOSTSYNC	15
MINSUPPLIES	1
MONITOR		<%= upsname %>@<%= mastername %> <%= numsupplies %> <%= username %> <%= password %> <%= mode %>
NOCOMMWARNTIME	300
POLLFREQ	10
POLLFREQALERT	5
POWERDOWNFLAG	/etc/killpower
RBWARNTIME	43200
SHUTDOWNCMD	"<%= shutdowncmd %>"

<% if upssched == "true" %>
NOTIFYCMD	/usr/local/bin/upssched
NOTIFYFLAG	LOWBATT SYSLOG+WALL+EXEC
NOTIFYFLAG	ONBATT SYSLOG+EXEC
NOTIFYFLAG	ONLINE SYSLOG+EXEC
<% end %>

